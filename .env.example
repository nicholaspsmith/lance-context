# lance-context Environment Variables
# Copy this file to .env and fill in your values

# Jina AI API key (optional - highest priority embedding backend)
# Get yours at: https://jina.ai/embeddings/
JINA_API_KEY=jina_...

# Ollama server URL (optional - local fallback)
# Default: http://localhost:11434
OLLAMA_URL=http://localhost:11434

# Project path to index (optional)
# Default: current working directory
LANCE_CONTEXT_PROJECT=/path/to/your/project
